```turtle
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://localhost/onto#> .

:StazioneAppaltante a owl:Class ;
    rdfs:label "Stazione appaltante"^^xsd:string ;
    rdfs:comment "Entità che procede all'acquisizione di forniture, servizi e lavori."^^xsd:string .

:CentraleDiCommittenzaQualificata a owl:Class ;
    rdfs:label "Centrale di committenza qualificata"^^xsd:string ;
    rdfs:comment "Entità che fornisce strumenti di acquisto messi a disposizione delle stazioni appaltanti."^^xsd:string .

:SoggettoAggregatore a owl:Class ;
    rdfs:label "Soggetto aggregatore"^^xsd:string ;
    rdfs:comment "Entità che fornisce strumenti di acquisto messi a disposizione delle stazioni appaltanti."^^xsd:string .

:StrumentoDiAcquistoENegoziazione a owl:Class ;
    rdfs:label "Strumento di acquisto e negoziazione"^^xsd:string ;
    rdfs:comment "Strumento utilizzato per contenere la spesa."^^xsd:string .

:AmbitoDellAcquisizione a owl:Class ;
    rdfs:label "Ambito dell'acquisizione"^^xsd:string ;
    rdfs:comment "L'ambito dell'acquisizione è quello delle forniture, servizi e lavori."^^xsd:string .

:SogliaPerGliAffidamentiDiretti a owl:Class ;
    rdfs:label "Soglia per gli affidamenti diretti"^^xsd:string ;
    rdfs:comment "Soglia prevista per gli affidamenti diretti non specificata nel testo."^^xsd:string .

:AcquisizioneDiretaDiFornitureEServizi a owl:Class ;
    rdfs:label "Acquisizione diretta di forniture e servizi"^^xsd:string ;
    rdfs:comment "Processo di acquisizione di forniture e servizi."^^xsd:string .

:AffidamentoDiLavori a owl:Class ;
    rdfs:label "Affidamento di lavori"^^xsd:string ;
    rdfs:comment "Processo di affidamento di lavori."^^xsd:string .

:UtilizzoDiStrumentiDiAcquistoENegoziazione a owl:Class ;
    rdfs:label "Utilizzo di strumenti di acquisto e negoziazione"^^xsd:string ;
    rdfs:comment "Uso di strumenti di acquisto e negoziazione."^^xsd:string .

:ResponsabileDi a owl:ObjectProperty ;
    rdfs:domain :StazioneAppaltante ;
    rdfs:range :AcquisizioneDiretaDiFornitureEServizi, :AffidamentoDiLavori ;
    rdfs:label "è responsabile di"^^xsd:string ;
    rdfs:comment "Relazione che indica la responsabilità della stazione appaltante su determinati processi."^^xsd:string .

:HaRuolo a owl:ObjectProperty ;
    rdfs:domain :StazioneAppaltante ;
    rdfs:range :StazioneAppaltante ;
    rdfs:label "ha ruolo"^^xsd:string ;
    rdfs:comment "Relazione che indica il ruolo della stazione appaltante."^^xsd:string .

:FornisceStrumentiDiAcquistoA a owl:ObjectProperty ;
    rdfs:domain :CentraleDiCommittenzaQualificata, :SoggettoAggregatore ;
    rdfs:range :StazioneAppaltante ;
    rdfs:label "fornisce strumenti di acquisto a"^^xsd:string ;
    rdfs:comment "Relazione che indica la fornitura di strumenti di acquisto da parte di entità qualificate."^^xsd:string .

:PrevistoDalleVigentiDisposizioniInMateriaDiContenimentoDellaSpesa a owl:ObjectProperty ;
    rdfs:domain :StrumentoDiAcquistoENegoziazione ;
    rdfs:range :UtilizzoDiStrumentiDiAcquistoENegoziazione ;
    rdfs:label "è previsto dalle vigenti disposizioni in materia di contenimento della spesa"^^xsd:string ;
    rdfs:comment "Relazione che indica la previsione delle disposizioni vigenti sulla utilizzazione di strumenti di acquisto e negoziazione."^^xsd:string .

:SogliaNonSpecificataNelTesto a owl:ObjectProperty ;
    rdfs:domain :SogliaPerGliAffidamentiDiretti ;
    rdfs:range :SogliePerGliAffidamentiDiretti ;
    rdfs:label "non è specificata nel testo"^^xsd:string ;
    rdfs:comment "Relazione che indica che la soglia per gli affidamenti diretti non è specificata nel testo."^^xsd:string .
```